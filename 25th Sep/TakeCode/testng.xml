<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite">
  <listeners>
<listener class-name="org.uncommons.reportng.HTMLReporter" />
</listeners>
  <test name="User Login">
    <classes>
      <class name="framework.TakeCode.Login"/>
    </classes>
  </test> 
  <test name="Admin Login">
    <classes>
      <class name="framework.TakeCode.Admin_Login"/>
    </classes>
  </test> 
  <test name="User Logout">
    <classes>
      <class name="framework.TakeCode.Logout"/>
    </classes>
  </test> 
  <test name="User Registration">
    <classes>
      <class name="framework.TakeCode.Registration"/>
    </classes>
  </test> 
  <test name="User Ask Question">
    <classes>
      <class name="framework.TakeCode.Ask_Question"/>
    </classes>
  </test> 
</suite> 
